alias dev="cd ~/dev"
alias a="argocd"
alias g="git"
alias k="kubectl"
alias t="terraform"
alias d="docker"
alias serve-http="http-server ./ -p 3000 -c-1 --silent"
alias gup="git fetch origin && git merge origin/master master && git checkout master && git pull && g-clean"
alias g-clean="git remote prune origin && git branch --merged | grep -v \"\*\" | grep -v master | grep -v dev | xargs -n 1 git branch -d"
alias g-clean-remote="git branch -r --merged | grep -v master | sed 's/origin\///' | xargs -n 1 git push --delete origin"
alias gbrD="git push -d origin"
alias disk-usage="du -hsx * | sort -n -r"
alias file-size='ls -hlS'
alias docker-kill-all='docker rm -f $(docker ps -a -q)'
alias n="npm"
alias nx="npx"
alias r="rustc"
alias c="cargo"
# alias n="pnpm"
alias edit_hosts="sudo nano /etc/hosts"
alias reset_dns="sudo killall -HUP mDNSResponder"
alias jsonun="pbpaste | jq fromjson | pbcopy"